{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "esnext",
        "target": "es2015",
        "outDir": "./public",
        "jsx": "react-jsxdev",
        "strict": true,
        "sourceMap": true,
        "incremental": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "noFallthroughCasesInSwitch": true
    }
}
